{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/infinite-runner/(.*)",
      "destination": "/infinite-runner/$1"
    },
    {
      "source": "/city-view/(.*)",
      "destination": "/city-view/$1"
    }
  ]
}
